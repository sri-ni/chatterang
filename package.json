{
  "name": "savantory-books",
  "version": "0.0.1",
  "description": "Microservice for Books data with CRUD capabilities for admin.",
  "main": "index.js",
  "scripts": {
    "start": "npm run apidoc && babel-node index.js",
    "test": "NODE_ENV=test mocha test/**/*.js",
    "apidoc": "apidoc -i routes/ -o public/apidoc"
  },
  "apidoc": {
    "name": "savantory-books API Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "keywords": [
    "books",
    "reading"
  ],
  "author": "Thirumalaa Srinivas",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.2",
    "consign": "^0.1.2",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "jwt-simple": "^0.4.1",
    "morgan": "^1.6.1",
    "mysql": "^2.11.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "sequelize": "^3.19.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  }
}
